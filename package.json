    "rehype-code-titles": "1.2.0",
    "rehype-slug": "6.0.0",
    "shiki": "3.11.0",
    "valibot": "1.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.4",
    "@next/bundle-analyzer": "15.5.0",
    "@opennextjs/aws": "3.7.5",
    "@opennextjs/cloudflare": "1.6.5",
    "@tailwindcss/postcss": "4.1.12",
    "@textlint/textlint-plugin-markdown": "15.2.2",
    "@types/gtag.js": "0.0.20",
    "@types/markdown-it": "14.1.2",
    "@types/mdx": "2.0.13",
    "@types/node": "22.15.32",
    "@types/react": "19.1.11",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.12",
    "textlint": "15.2.2",
    "textlint-rule-preset-ja-spacing": "2.4.3",
    "textlint-rule-preset-ja-technical-writing": "12.0.2",
    "textlint-rule-spellcheck-tech-word": "5.0.0",
    "typescript": "5.9.2",
    "wrangler": "4.29.1"
  },
  "packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92",
  "scripts": {
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "dev": "next dev --turbopack",
    "start": "next start",
    "lint": "biome check",
    "lint:post": "textlint _posts/**/*.mdx",
    "format": "biome check --write",
    "typecheck": "tsc --noEmit",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview -- --minify",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy -- --minify",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload -- --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "nextBundleAnalysis": {
    "budget": 358400,
    "budgetPercentIncreaseRed": 20,
    "showDetails": true
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "workerd"
    ]
  }
}
