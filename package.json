    "@fortawesome/free-solid-svg-icons": "7.0.0",
    "@fortawesome/react-fontawesome": "0.2.6",
    "@mdx-js/mdx": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@shikijs/langs": "3.11.0",
    "@shikijs/rehype": "3.11.0",
    "@shikijs/themes": "3.11.0",
