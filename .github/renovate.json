{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "labels": ["dependencies"],
  "prHourlyLimit": 5,
  "prConcurrentLimit": 20,
  "major": {
    "stabilityDays": 7
  },
  "minor": {
    "stabilityDays": 3
  },
  "patch": {
    "stabilityDays": 2,
    "automerge": true,
    "labels": ["dependencies", "automerge: enabled"]
  }
}
